{
  "expo": {
    "name": "TruckSarthi",
    "slug": "trucksarthi-frontend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/TruckSarthi-Logo.png",
    "scheme": "trucksarthifrontend",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sahilgumber7.trucksarthi",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.sahilgumber7.trucksarthi",
      "versionCode": 29,
      "permissions": [
        "android.permission.INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TruckSarthi-Logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/TruckSarthi-Graphic.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/TruckSarthi-Graphic.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1196740f-77b4-45d2-a85e-ec3b44fd5d74"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": false
    }
  }
}