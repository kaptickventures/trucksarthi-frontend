{
  "expo": {
    "name": "TruckSarthi",
    "slug": "trucksarthi-frontend",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/TruckSarthi-Logo.png",
    "scheme": "trucksarthifrontend",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trucksarthi.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.trucksarthi.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TruckSarthi-Logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "trucksarthifrontend",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/TruckSarthi-Graphic.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/TruckSarthi-Graphic.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "092c3436-79f6-481d-968d-549b9116286e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "trucksarthi01",
    "updates": {
      "url": "https://u.expo.dev/092c3436-79f6-481d-968d-549b9116286e"
    }
  }
}
